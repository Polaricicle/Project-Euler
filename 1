allnum = []
total = 0
for j in range(3, 1000, 3): #check for all multiples of 3
    allnum.append(j)
for k in range(5, 1000, 5): #check for all multiples of 5
    if k not in allnum: #check for repeated multiples (i.e. multiples of both 3 and 5)
        allnum.append(k)

for i in range(0, len(allnum)): #calculate the total value of all multiples
    total += allnum[i]

print(total)
